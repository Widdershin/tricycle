{
  "name": "tricycle",
  "version": "0.0.1",
  "description": "A scratchpad for trying out Cycle.js",
  "main": "lib/scratchpad.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "start": "babel-node server.js",
    "test": "mocha --compilers js:babel-core/register",
    "precompile-lib": "rm -rf lib/ && mkdir -p lib",
    "compile-lib": "babel src -d lib",
    "prepublish": "npm run compile-lib",
    "bundle": "browserify index.js -o bundle.js -t babelify"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Widdershin/trycycle.git"
  },
  "keywords": [
    "cycle",
    "cycle.js",
    "FRP",
    "happiness"
  ],
  "author": "Nick Johnstone",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Widdershin/trycycle/issues"
  },
  "homepage": "https://github.com/Widdershin/trycycle",
  "dependencies": {
    "@cycle/core": "^6.0.0-rc2",
    "@cycle/dom": "^8.0.0-rc2",
    "babel-core": "^6.2.1",
    "babel-preset-es2015": "^6.1.18",
    "brace": "^0.5.1",
    "cycle-restart": "0.0.11",
    "lodash": "^3.10.1",
    "rx": "^4.0.7"
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "budo": "^6.0.4",
    "garnish": "^4.1.1",
    "mocha": "^2.3.4"
  }
}
